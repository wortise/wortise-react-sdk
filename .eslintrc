{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "plugin:prettier/recommended", 
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "settings": {
    "react": {
      "version": "16.2.0"
    }
  }
}